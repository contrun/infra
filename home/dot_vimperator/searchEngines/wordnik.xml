<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>wordnik</os:ShortName>
<os:Description>Wordnik</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://www.wordnik.com</SearchForm>
<os:Url type="text/html" method="GET" template="https://www.wordnik.com/words" resultDomain="wordnik.com">
  <os:Param name="myWord" value="{searchTerms}"/>
  <os:Param name="commit" value="I+always+feel+lucky"/>
</os:Url>
</SearchPlugin>