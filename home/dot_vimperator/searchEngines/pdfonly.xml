<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>pdfonly</os:ShortName>
<os:Description>Search documents in Adobe PDF format with Google</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/gif;base64,</os:Image>
<SearchForm>http://www.google.com</SearchForm>
<os:Url type="text/html" method="GET" template="http://www.google.com/search?q={searchTerms}+filetype:pdf" resultDomain="google.com">
</os:Url>
<os:Url type="application/opensearchdescription+xml" method="GET" template="http://sitime.ru/files/search-plugin/onlypdf.xml" rel="self" resultDomain="sitime.ru">
</os:Url>
</SearchPlugin>