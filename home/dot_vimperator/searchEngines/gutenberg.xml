<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>gutenberg</os:ShortName>
<os:Description>Search for public domain e-books in Project Gutenberg</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/vnd.microsoft.icon;base64,</os:Image>
<SearchForm>http://www.gutenberg.org</SearchForm>
<os:Url type="text/html" method="POST" template="http://www.gutenberg.org/catalog/world/results" resultDomain="gutenberg.org">
  <os:Param name="title" value="{searchTerms}"/>
</os:Url>
</SearchPlugin>