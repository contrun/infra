<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>yandex</os:ShortName>
<os:Description>Search Yandex.com in English</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>https://www.yandex.com/search</SearchForm>
<os:Url type="text/html" method="GET" template="https://www.yandex.com/search/?text={searchTerms}" resultDomain="yandex.com">
</os:Url>
<os:Url type="application/x-suggestions+json" method="GET" template="https://suggest.yandex.com/suggest-ff.cgi?uil=en&amp;part={searchTerms}" rel="suggestions" resultDomain="suggest.yandex.com">
</os:Url>
<os:Url type="application/x-moz-keywordsearch" method="GET" template="https://www.yandex.com/search/?text={searchTerms}" rel="moz-keywordsearch" resultDomain="yandex.com">
</os:Url>
</SearchPlugin>