<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>googlescholar</os:ShortName>
<os:Description>Use Google scholar in search bar</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>https://scholar.google.com</SearchForm>
<os:Url type="text/html" method="GET" template="https://scholar.google.com/scholar?q={searchTerms}" resultDomain="scholar.google.com">
</os:Url>
<os:Url type="application/x-suggestions+json" method="GET" template="https://scholar.google.com/scholar?q={searchTerms}" resultDomain="scholar.google.com">
</os:Url>
</SearchPlugin>