<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>wiktionaryfr</os:ShortName>
<os:Description>Le dictionnaire libre et gratuit que chacun peut am√©liorer</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>https://fr.wiktionary.org/wiki/Special:Search</SearchForm>
<os:Url type="application/x-suggestions+json" method="GET" template="https://fr.wiktionary.org/w/api.php" resultDomain="fr.wiktionary.org">
  <os:Param name="action" value="opensearch"/>
  <os:Param name="search" value="{searchTerms}"/>
</os:Url>
<os:Url type="text/html" method="GET" template="https://fr.wiktionary.org/wiki/Special:Search" resultDomain="fr.wiktionary.org">
  <os:Param name="search" value="{searchTerms}"/>
  <os:Param name="sourceid" value="Mozilla-search"/>
</os:Url>
</SearchPlugin>