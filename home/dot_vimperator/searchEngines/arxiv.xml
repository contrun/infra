<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>arxiv</os:ShortName>
<os:Description>arXiv</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://arxiv.org</SearchForm>
<os:Url type="text/html" method="POST" template="https://arxiv.org/search" resultDomain="arxiv.org">
  <os:Param name="query" value="{searchTerms}"/>
  <os:Param name="searchtype" value="all"/>
</os:Url>
</SearchPlugin>