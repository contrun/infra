<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>britannica</os:ShortName>
<os:Description>Encyclopedia Britannica</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>http://www.britannica.com</SearchForm>
<os:Url type="text/html" method="GET" template="http://www.britannica.com/search" resultDomain="britannica.com">
