<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>amo</os:ShortName>
<os:Description>Searches addons on addons.mozilla.org</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>https://addons.mozilla.org/-/firefox/search</SearchForm>
<os:Url type="text/html" method="GET" template="https://addons.mozilla.org/-/firefox/search" resultDomain="addons.mozilla.org">
  <os:Param name="cat" value="all"/>
  <os:Param name="q" value="{searchTerms}"/>
</os:Url>
<os:Url type="application/x-suggestions+json" method="GET" template="https://addons.mozilla.org/-/firefox/search?cat=all&amp;q={searchTerms}" resultDomain="addons.mozilla.org">
</os:Url>
</SearchPlugin>