<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>wikipedia</os:ShortName>
<os:Description>English Wikipedia with SSL protection (en.wikipedia.org for best performance)</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>https://en.wikipedia.org/wiki/</SearchForm>
<os:Url type="text/html" method="GET" template="https://en.wikipedia.org/wiki/Special:Search?go=Go&amp;search={searchTerms}" resultDomain="en.wikipedia.org">
</os:Url>
<os:Url type="application/x-suggestions+json" method="GET" template="https://en.wikipedia.org/w/api.php?action=opensearch&amp;search={searchTerms}" resultDomain="en.wikipedia.org">
</os:Url>
<os:Url type="application/opensearchdescription+xml" method="GET" template="http://mycroftproject.com/updateos.php/id0/wikipedia_en_ssl.xml" rel="self" resultDomain="mycroftproject.com">
</os:Url>
</SearchPlugin>